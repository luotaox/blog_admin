{"ast":null,"code":"import { ArrowRight, Edit, Divider, Position, Document } from '@element-plus/icons-vue';\nimport { ref, onMounted, reactive } from 'vue';\nimport axios from 'axios';\nimport { ElMessage } from 'element-plus';\nexport default {\n  __name: 'Cate',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    let cateData = ref([]);\n    const addRef = ref();\n    let editCateVisible = ref(false);\n    let cateForm = reactive({\n      title: '',\n      href: '',\n      _id: ''\n    });\n    let addForm = reactive({\n      title: '',\n      href: ''\n    });\n\n    // 验证规则\n    const cateRules = reactive({\n      title: [{\n        trigger: 'blur',\n        required: true,\n        message: 'title不能为空'\n      }],\n      href: [{\n        trigger: 'blur',\n        required: true,\n        message: 'href不能为空'\n      }]\n    });\n    const getCateList = async () => {\n      const res = await axios.get('/adminapi/category/list');\n      if (res.status !== 201) return ElMessage.error('获取列表失败');\n      cateData.value = res.data.data;\n    };\n    const cateEdit = data => {\n      Object.assign(cateForm, data);\n      editCateVisible.value = true;\n    };\n    const editSubmit = async () => {\n      const res = await axios.put('/adminapi/category', cateForm);\n      if (res.status !== 201) return ElMessage.error('修改失败');\n      editCateVisible.value = false;\n      ElMessage.success('修改成功');\n    };\n    const close = () => {\n      editCateVisible.value = false;\n      ElMessage.info('已取消');\n    };\n    const addSubmit = async () => {\n      addRef.value.validate(async valid => {\n        if (!valid) return ElMessage.error('请填写必要表单项');\n        const res = await axios.post('/adminapi/category/add', addForm);\n        if (res.status !== 201) return ElMessage.error('添加失败');\n        ElMessage.success('添加成功');\n        getCateList();\n      });\n    };\n    onMounted(() => {\n      getCateList();\n    });\n    const __returned__ = {\n      get cateData() {\n        return cateData;\n      },\n      set cateData(v) {\n        cateData = v;\n      },\n      addRef,\n      get editCateVisible() {\n        return editCateVisible;\n      },\n      set editCateVisible(v) {\n        editCateVisible = v;\n      },\n      get cateForm() {\n        return cateForm;\n      },\n      set cateForm(v) {\n        cateForm = v;\n      },\n      get addForm() {\n        return addForm;\n      },\n      set addForm(v) {\n        addForm = v;\n      },\n      cateRules,\n      getCateList,\n      cateEdit,\n      editSubmit,\n      close,\n      addSubmit,\n      get ArrowRight() {\n        return ArrowRight;\n      },\n      get Edit() {\n        return Edit;\n      },\n      get Divider() {\n        return Divider;\n      },\n      get Position() {\n        return Position;\n      },\n      get Document() {\n        return Document;\n      },\n      ref,\n      onMounted,\n      reactive,\n      get axios() {\n        return axios;\n      },\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ArrowRight","Edit","Divider","Position","Document","ref","onMounted","reactive","axios","ElMessage","cateData","addRef","editCateVisible","cateForm","title","href","_id","addForm","cateRules","trigger","required","message","getCateList","res","get","status","error","value","data","cateEdit","Object","assign","editSubmit","put","success","close","info","addSubmit","validate","valid","post"],"sources":["C:/Users/86191/Desktop/admin/src/views/essay/Cate.vue"],"sourcesContent":["<template>\r\n  <div class=\"categories\">\r\n    <!-- 面包屑区域 -->\r\n    <div class=\"bread\">\r\n      <el-breadcrumb :separator-icon=\"ArrowRight\">\r\n        <el-breadcrumb-item>文章管理</el-breadcrumb-item>\r\n        <el-breadcrumb-item>分类管理</el-breadcrumb-item>\r\n      