Submit = async () => {\r\n  addRef.value.validate(async (valid) => {\r\n    if (!valid) return ElMessage.error('请填写必要表单项');\r\n    const res = await axios.post('/adminapi/category/add', addForm);\r\n    if (res.status !== 201) return ElMessage.error('添加失败');\r\n    ElMessage.success('添加成功');\r\n    getCateList();\r\n  })\r\n}\r\n\r\nonMounted(() => {\r\n  getCateList();\r\n})\r\n</script>"],"mappings":"AA0EA,SAASA,UAAU,EAAEC,IAAI,EAAEC,OAAO,EAAEC,QAAQ,EAAEC,QAAQ,QAAQ,yBAAyB;AACvF,SAASC,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,SAAS,QAAQ,cAAc;;;;;;;IAExC,IAAIC,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMM,MAAM,GAAGN,GAAG,CAAC,CAAC;IAEpB,IAAIO,eAAe,GAAGP,GAAG,CAAC,KAAK,CAAC;IAEhC,IAAIQ,QAAQ,GAAGN,QAAQ,CAAC;MACtBO,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,GAAG,EAAE;IACP,CAAC,CAAC;IAEF,IAAIC,OAAO,GAAGV,QAAQ,CAAC;MACrBO,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE;IACR,CAAC,CAAC;;IAEF;IACA,MAAMG,SAAS,GAAGX,QAAQ,CAAC;MACzBO,KAAK,EAAE,CACL;QACEK,OAAO,EAAE,MAAM;QACfC,QAAQ,EAAE,IAAI;QACdC,OAAO,EAAE;MACX,CAAC,CACF;MACDN,IAAI,EAAE,CACJ;QACEI,OAAO,EAAE,MAAM;QACfC,QAAQ,EAAE,IAAI;QACdC,OAAO,EAAE;MACX,CAAC;IAEL,CAAC,CAAC;IAEF,MAAMC,WAAW,GAAG,MAAAA,CAAA,KAAY;MAC9B,MAAMC,GAAG,GAAG,MAAMf,KAAK,CAACgB,GAAG,CAAC,yBAAyB,CAAC;MACtD,IAAID,GAAG,CAACE,MAAM,KAAK,GAAG,EAAE,OAAOhB,SAAS,CAACiB,KAAK,CAAC,QAAQ,CAAC;MACxDhB,QAAQ,CAACiB,KAAK,GAAGJ,GAAG,CAACK,IAAI,CAACA,IAAI;IAChC,CAAC;IAED,MAAMC,QAAQ,GAAID,IAAI,IAAK;MACzBE,MAAM,CAACC,MAAM,CAAClB,QAAQ,EAAEe,IAAI,CAAC;MAC7BhB,eAAe,CAACe,KAAK,GAAG,IAAI;IAC9B,CAAC;IAED,MAAMK,UAAU,GAAG,MAAAA,CAAA,KAAY;MAC7B,MAAMT,GAAG,GAAG,MAAMf,KAAK,CAACyB,GAAG,CAAC,oBAAoB,EAAEpB,QAAQ,CAAC;MAC3D,IAAIU,GAAG,CAACE,MAAM,KAAK,GAAG,EAAE,OAAOhB,SAAS,CAACiB,KAAK,CAAC,MAAM,CAAC;MACtDd,eAAe,CAACe,KAAK,GAAG,KAAK;MAC7BlB,SAAS,CAACyB,OAAO,CAAC,MAAM,CAAC;IAC3B,CAAC;IAED,MAAMC,KAAK,GAAGA,CAAA,KAAM;MAClBvB,eAAe,CAACe,KAAK,GAAG,KAAK;MAC7BlB,SAAS,CAAC2B,IAAI,CAAC,KAAK,CAAC;IACvB,CAAC;IAED,MAAMC,SAAS,GAAG,MAAAA,CAAA,KAAY;MAC5B1B,MAAM,CAACgB,KAAK,CAACW,QAAQ,CAAC,MAAOC,KAA