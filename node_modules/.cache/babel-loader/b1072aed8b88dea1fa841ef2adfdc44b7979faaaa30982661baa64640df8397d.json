rcle @click=\"hanldPreview(scope.row)\" />\r\n              </el-tooltip>\r\n              <el-tooltip effect=\"light\" content=\"编辑\" placement=\"bottom\" :enterable=\"false\">\r\n                <el-button type=\"primary\" :icon=\"Edit\" circle />\r\n              </el-tooltip>\r\n              <el-tooltip effect=\"light\" content=\"删除\" placement=\"bottom\" :enterable=\"false\">\r\n                <el-button type=\"danger\" :icon=\"Delete\" circle />\r\n              </el-tooltip>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<!-- 预览对话框 -->\r\n<div class=\"dialog\">\r\n   <el-dialog\r\n    v-model=\"dialogVisible\"\r\n    title=\"Tips\"\r\n    width=\"30%\"\r\n    :before-close=\"handleClose\"\r\n  >\r\n    <span>This is a message</span>\r\n    <template #footer>\r\n      <span class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\">\r\n          Confirm\r\n        </el-button>\r\n      </span>\r\n    </template>\r\n  </el-dialog>\r\n</div>\r\n<script setup>\r\nimport { ArrowRight, Edit, Star, Delete } from '@element-plus/icons-vue'\r\nimport { ref, onMounted } from 'vue'\r\nimport axios from 'axios'\r\nimport formatTime from '@/util/formantTime'\r\n\r\nconst tableData = ref([]);\r\n// 预览数据\r\nconst previewList = ref({})\r\n\r\nonMounted(() => {\r\n  getTableData();\r\n})\r\n\r\n// 获取新闻数据\r\nconst getTableData = async () => {\r\n  const res = await axios.get(`adminapi/news/list`);\r\n  if (res.status !== 201) return ElMessage.error('获取新闻列表失败');\r\n  tableData.value = res.data.data\r\n}\r\n\r\n// 格式化分类数据\r\nconst categoryFormat = (category) => {\r\n  const Arr = ['最新发布', '典型案例', '通知公告'];\r\n  return Arr[category - 1];\r\n}\r\n\r\n// 发布关闭新闻\r\nconst hanldSwitchChange = async (data) => {\r\n  const res = await axios.put(`/adminapi/news/publish/`, {\r\n    _id: data._id,\r\n    isPublish: data.isPublish\r\n  });\r\n  if (res.status !== 201) return ElMessage.error('修改失败');\r\n  ElMessage.success('修改成功')\r\n}\r\n\r\n// 监听预览事件\r\nconst hanldPreview = (data) => {\r\n  previewList.value = data;\r\n  console.log(previewList.value = data);\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped></style>"],"mappings":"AA2EA,SAASA,UAAU,EAAEC,IAAI,EAAEC,IAAI,EAAEC,MAAM,QAAQ,yBAAyB;AACxE,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,UAAU,MAAM,oBAAoB;;;;;;;IAE3C,MAAMC,SAAS,GAAGJ,GAAG,CAAC,EAAE,CAAC;IACzB;IACA,MAAMK,WAAW,GAAGL,GAAG,CAAC,CAAC,CAAC,CAAC;IAE3BC,SAAS,CAAC,MAAM;MACdK,YAAY,CAAC,CAAC;IAChB,CAAC,CAAC;;IAEF;IACA,MAAMA,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/B,MAAMC,GAAG,GAAG,MAAML,KAAK,CAACM,GAAG,CAAE,oBAAmB,CAAC;MACjD,IAAID,GAAG,CAACE,MAAM,KAAK,GAAG,EAAE,OAAOC,SAAS,CAACC,KAAK,CAAC,UAAU,CAAC;MAC1DP,SAAS,CAACQ,KAAK,GAAGL,GAAG,CAACM,IAAI,CAACA,IAAI;IACjC,CAAC;;IAED;IACA,MAAMC,cAAc,GAAIC,QAAQ,IAAK;MACnC,MAAMC,GAAG,GAAG,CAAC,MAAM,EAAE,MAAM,EAAE,MAAM,CAAC;MACpC,OAAOA,GAAG,CAACD,QAAQ,GAAG,CAAC,CAAC;IAC1B,CAAC;;IAED;IACA,MAAME,iBAAiB,GAAG,MAAOJ,IAAI,IAAK;MACxC,MAAMN,GAAG,GAAG,MAAML,KAAK,CAACgB,GAAG,CAAE,yBAAwB,EAAE;QACrDC,GAAG,EAAEN,IAAI,CAACM,GAAG;QACbC,SAAS,EAAEP,IAAI,CAACO;MAClB,CAAC,CAAC;MACF,IAAIb,GAAG,CAACE,MAAM,KAAK,GAAG,EAAE,OAAOC,SAAS,CAACC,KAAK,CAAC,MAAM,CAAC;MACtDD,SAAS,CAACW,OAAO,CAAC,MAAM,CAAC;IAC3B,CAAC;;IAED;IACA,MAAMC,YAAY,GAAIT,IAAI,IAAK;MAC7BR,WAAW,CAACO,KAAK,GAAGC,IAAI;MACxBU,OAAO,CAACC,GAAG,CAACnB,WAAW,CAACO,KAAK,GAAGC,IAAI,CAAC;IACvC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}